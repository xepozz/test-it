{
  "name": "xepozz/test-it",
  "bin": ["test-it"],
  "require": {
    "nette/php-generator": "^3.6.9",
    "nikic/php-parser": "^4.15",
    "psr/container": "^2.0",
    "psr/log": "^3.0",
    "symfony/console": "^6.2",
    "symfony/finder": "^5|^6",
    "yiisoft/di": "^1.2",
    "yiisoft/event-dispatcher": "^1.1",
    "yiisoft/var-dumper": "^1.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.28"
  },
  "autoload": {
    "psr-4": {
      "Xepozz\\TestIt\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "autoload-dev": {
    "psr-4": {
      "Xepozz\\TestIt\\Tests\\": "tests"
    }
  }
}
